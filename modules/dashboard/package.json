{
  "name": "drone-network-dashboard",
  "version": "1.0.0",
  "description": "single page app showing a list of drones known to the net-ops-center",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm install && cd ./client && npm install && npm run build",
    "client": "NODE_ENV=development cd client && npm start",
    "server": "NODE_ENV=development node server.js",
    "dev": "NODE_ENV=development concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "prod": "NODE_ENV=production node server.js"
  },
  "author": "dt-rush",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "yarn": "^1.7.0"
  }
}
